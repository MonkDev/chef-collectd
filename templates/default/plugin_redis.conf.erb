LoadPlugin "<%= @name %>"

<% if not @config.empty? %>
<Plugin "<%= @name %>">
  <Node "mynode">
  <% @config.each_pair do |key, value|
    if value.is_a? Array
      value.each do |subvalue| %>
    <%= key %> <%= collectd_option(subvalue) %>
  <% end else %>
    <%= key %> <%= collectd_option(value) %>
  <% end end %>
  </Node>
</Plugin>
<% end %>
